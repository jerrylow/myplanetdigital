<!DOCTYPE html>
<!--[if lt IE 8 ]>    <html lang="en" class="ie7"> <![endif]-->
<!--[if IE 8 ]>    <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9 ]>    <html lang="en" class="ie9"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html lang="en"> <!--<![endif]-->
	<head>
		<meta charset="utf-8">
		<script>
			 (new Image()).src = "/images/loading.gif";
		</script>
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=0">
        <!--[if (lt IE 8) ]><meta http-equiv="refresh" content="0;url=<%=@getUrl('/upgrade')%>"> <![endif]-->
		<% if @document.title?: %>
			<title><%= @document.title %> | <%= @site.title %></title>
		<% else: %>
			<title><%= @site.title %></title>
		<% end %>
		<% if @document.summary? or @document.menuBanner?: %>
			<meta name="description" content="<%- @document.summary || @document.menuBanner.replace(/<(.|\n)*?>/ig, '') %>">
		<% end %>
		<%- @getBlock("meta").toHTML() %>
		<!--[if !IE]> -->
		<%- @getBlock('styles').add(@getUrl(@site.styles)).toHTML() %>
		<!-- <![endif]-->

		<!--[if (lt IE 8)]>
			<link rel="stylesheet" href="<%- @getUrl(@site.ie7styles[0]) %>">
		<![endif]-->

		<!--[if (IE 8) | (IE 9)]>
		<link rel="stylesheet" href="<%- @getUrl(@site.ie8styles[0]) %>">
		<script src="<%- @getUrl(@site.html5shiv[0]) %>"></script>
		<script>
			window.html5 = {
				'elements': 'section article footer',
				'shivCSS': false,
				'shivMethods': false
			}
			window.isIE8 = true;
		</script>
		<![endif]-->
		<link rel="icon" type="image/png" href="<%= @getUrl('/favicon.png') %>">
		<% url = @document.url %>
		<% if url == '/': %>
			<% url = '' %>
		<% else: %>
			<% url = url + '/' %>
		<% end %>
		<link rel="canonical" href="http://myplanet.io<%= url %>">
	</head>

	<body class="<% if @document.layout != 'articles': %>article<% end %><% if typeof @document.slug == 'string' && @document.slug.length > 0 : %> <%= @document.slug %><% end %>">
		<div id="wrap">
			<h1 id="logo">
				<a href="<%= @getUrl('/') %>"><%= @site.title %> <% if @document.title?: %> | <%= @document.title %><% end %></a>
			</h1>
			<%- @partial('menu', {menuItems: @generateMenu(@document.url, 'menu')}) %>
			<div id="viewport">
				<section id="main" <% if @document.layout != 'articles': %>style="transition: none; -webkit-transition: none; position: absolute;"<% end %>>
					<div id="banner" class="<% if @document.meta.tagName?: %><%- @document.meta.tagName %><% end %>">
						<div class="banner-text">
						<% if @document.meta.menuBanner?: %>
					  		<%- @document.meta.menuBanner %>
						<% end %>
						</div>
					</div>
					<div class="main-wrap" style="position: relative;">
						<% if @document.layout is 'articles': %>
						<div class="grid-size"></div>
						<%- @content %>
						<% end %>
						<script src="<%- @getUrl(@site.isotope[0]) %>"></script>
						<script>
						<% if @document.layout != 'articles': %>
							window.initializeTiles = function(){
						<% end %>
						    <%- @include(@site['tiles-immediate'][0]) %>
						<% if @document.layout != 'articles': %>
							};
						<% end %>
						</script>
					</div>
					<div class="loading-overlay-tiles" <% if @document.layout == 'articles': %>style="display:none;"<% end %>>
						<div class="loading-spinner"></div>
					</div>
					<%- @partial('footer') %>
				</section>
				<article id="article" style="<% if @document.layout != 'articles': %>position: static; margin-left: 100%;<% else: %>left: 100%;<% end %>">
					<div id="article-inner"  <% if @document.layout != 'content' && @document.layout != 'articles': %>class="reveal"<% end %>>
						<% if @document.layout != 'articles': %>
						<%- @content %>
						<% end %>
					</div>
					<div class="loading-overlay" <% if @document.layout != 'articles': %>style="display:none;"<% end %>>
						<div class="loading-spinner"></div>
					</div>
                    <%- @partial('footer') %>
				</article>
			</div>
		</div>
		<div id="back">
			<a href="<%= @getUrl('/') %>" data-attr="back">Back</a>
		</div>
		<!--[if (IE 8) | (IE 9)]>
		<script src="<%- @getUrl(@site.jquery1[0]) %>"></script>
		<script src="<%- @getUrl(@site['main-ie8'][0]) %>"></script>
		<script src="<%- @getUrl(@site['menu-ie8'][0]) %>"></script>
		<![endif]-->
		<!--[if !IE]> -->
		<% if @getEnvironment() == 'development': %>
			<script src="<%- @getUrl(@site.jquery2[0]) %>"></script>
			<%- @getBlock('scripts').add(@getUrl(@site.scripts)).toHTML() %>
		<% else: %>
			<script src="/scripts/all.js"></script>
		<% end %>
		<!-- <![endif]-->
		<% if @getEnvironment() != 'development': %>
		<%- @getBlock('scripts').toHTML() %>
		<% end %>
		<script>
		(function (tos) {
		  window.setInterval(function () {
		    tos = (function (t) {
		      return t[0] == 50 ? (parseInt(t[1]) + 1) + ':00' : (t[1] || '0') + ':' + (parseInt(t[0]) + 10);
		    })(tos.split(':').reverse());
		    debugger;
		    ga('send', 'event', 'Time', 'Log', tos);
		  }, 8000);
		})('00');
		</script>
		
	</body>
</html>
